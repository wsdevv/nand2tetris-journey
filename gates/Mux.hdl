/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notsel);
    And(a=b, b=sel, out=o);
    And(a=a, b=notsel, out=o2);
    Xor(a=o, b=o2, out=out);
}